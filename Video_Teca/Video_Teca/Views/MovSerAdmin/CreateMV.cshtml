@model Video_Teca.Models.MovSerGenAuModel


<h4>MoviesAndSeries</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="@Model.MovSer.media_type" class="control-label">Movie o Series</label>
                <select id="media_type" asp-for="@Model.MovSer.media_type" class="form-control">
                    <option value="movie">movie</option>
                    <option value="series">series</option>
                </select>
                <span asp-validation-for="@Model.MovSer.media_type" class="text-danger"></span>
            </div>
           
        </form>
        
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>